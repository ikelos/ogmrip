<profile name="default-ogm">
  <key name="name">'PC, Low Quality'</key>
  <key name="description">'1-pass, 1x700MB (OGM + Lavc + Vorbis)'</key>
  <section name="general">
    <key name="container">'ogm'</key>
    <key name="ensure-sync">false</key>
  </section>
  <section name="video">
    <key name="codec">'lavc-mpeg4'</key>
    <key name="passes">1</key>
    <key name="quality">0</key>
  </section>
  <section name="audio">
    <key name="codec">'vorbis'</key>
  </section>
  <section name="subp">
    <key name="codec">'srt'</key>
  </section>
</profile>
