<profile name="default-copy">
  <key name="name">'Copy'</key>
  <key name="description">'No reencoding'</key>
  <section name="general">
    <key name="container">'mkv'</key>
  </section>
  <section name="video">
    <key name="codec">'video-copy'</key>
    <key name="passes">1</key>
    <key name="scaler">0</key>
    <key name="can-crop">false</key>
  </section>
  <section name="audio">
    <key name="codec">'audio-copy'</key>
  </section>
  <section name="subp">
    <key name="codec">'vobsub'</key>
  </section>
</profile>
