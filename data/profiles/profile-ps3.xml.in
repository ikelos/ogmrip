<profile name="default-ps3">
  <key name="name">'Sony PS3'</key>
  <key name="description">'Quantizer 2 (MP4 + X264 + AAC)'</key>
  <section name="general">
    <key name="container">'mp4'</key>
    <key name="encoding-method">2</key>
  </section>
  <section name="video">
    <key name="codec">'x264'</key>
    <key name="expand">true</key>
    <key name="max-width">1920</key>
    <key name="max-height">1080</key>
    <key name="quality">3</key>
  </section>
  <section name="audio">
    <key name="codec">'aac'</key>
    <key name="channels">1</key>
  </section>
  <section name="subp">
    <key name="codec">'srt'</key>
  </section>
  <section name="x264">
    <key name="aud">true</key>
    <key name="b-adapt">2</key>
    <key name="b-frames">3</key>
    <key name="b-pyramid">0</key>
    <key name="cabac">true</key>
    <key name="dct8x8">true</key>
    <key name="frameref">3</key>
    <key name="global-header">true</key>
    <key name="level-idc">41</key>
    <key name="me">3</key>
    <key name="mixed-refs">true</key>
    <key name="profile">1</key>
    <key name="rc-lookahead">50</key>
    <key name="subq">6</key>
    <key name="trellis">1</key>
    <key name="vbv-bufsize">20000</key>
    <key name="vbv-maxrate">20000</key>
    <key name="weight-b">true</key>
  </section>
</profile>

