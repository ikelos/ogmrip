<profile name="default-ios">
  <key name="name">'Apple iOS'</key>
  <key name="description">'Quantizer 2 (MP4 + X264 + AAC)'</key>
  <section name="general">
    <key name="container">'mp4'</key>
    <key name="encoding-method">2</key>
  </section>
  <section name="video">
    <key name="codec">'x264'</key>
    <key name="max-width">960</key>
    <key name="max-height">640</key>
    <key name="quality">3</key>
  </section>
  <section name="audio">
    <key name="codec">'aac'</key>
    <key name="channels">1</key>
  </section>
  <section name="subp">
    <key name="codec">'srt'</key>
  </section>
  <section name="mp4">
    <key name="format">2</key>
  </section>
  <section name="x264">
    <key name="dct8x8">false</key>
    <key name="global-header">true</key>
    <key name="level-idc">31</key>
    <key name="profile">1</key>
  </section>
</profile>
